worker_processes auto;
user www-data;

events {
    worker_connections 1024;
}

http {
    include mime.types;
	default_type application/octet-stream;

    sendfile        on;
    keepalive_timeout  60;

    server {
        listen       80;
        server_name  _;

        location /test {
            content_by_lua_file /usr/local/openresty/nginx/conf/connect_redis.lua;
        }
    }
}

